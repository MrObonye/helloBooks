{
  "name": "hellobooks",
  "version": "8.3.0",
  "description": "A simple library app",
  "main": "server/app.js",
  "scripts": {
    "start": "babel-node server/bin/www.js",
    "start:dev": "nodemon server/bin/www --exec babel-node",
    "test": "mocha --watch --compilers js:babel-register server/test/**/*.spec.js",
    "seed": "babel-node server/seeders/index.js && exit 1",
    "seedjoin": "babel-node server/seeders/borrowedBook.js && exit 1",
    "seedTest": "npm run seed && npm run seedjoin && npm run test",
    "build": "babel server -d server/dist",
    "serve": "node server/dist/bin/www.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/segunolalive/helloBooks.git"
  },
  "keywords": [
    "react",
    "library"
  ],
  "author": "segun ola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/segunolalive/helloBooks/issues"
  },
  "homepage": " https://segunolalive.github.io/helloBooks/templates/",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "faker": "^4.1.0",
    "jsonwebtoken": "^7.4.2",
    "morgan": "^1.8.2",
    "pg": "^7.0.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.4.2",
    "sequelize-cli": "^2.8.0"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "mocha": "^3.5.0",
    "supertest": "^3.0.0",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
}
