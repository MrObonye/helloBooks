{
  "name": "hellobooks",
  "version": "1.0.0",
  "description": "A simple library app",
  "main": "dist/server/app.js",
  "scripts": {
    "serve": "babel-node server/bin/www.js",
    "start:client": "webpack-dev-server --hot",
    "start:dev": "nodemon server/bin/www --exec babel-node",
    "test:travis": "cross-env NODE_ENV=test nyc mocha -t 5000 --compilers js:babel-register server/test/**/*.spec.js",
    "pretest": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test && cross-env NODE_ENV=test npm run seed:tables",
    "test": "cross-env NODE_ENV=test npm run seed:join && npm run test:travis",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "seed:tables": "cross-env NODE_ENV=test babel-node server/seeders/user.js && babel-node server/seeders/bookCategory.js && babel-node server/seeders/book.js",
    "seed:join": "cross-env NODE_ENV=test babel-node server/seeders/borrowedBook.js",
    "build": "rm -rf dist && babel server -d dist/server && webpack",
    "start": "node dist/server/bin/www.js",
    "heroku-postbuild": "npm run build"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "dist"
    ]
  },
  "engines": {
    "node": 6,
    "npm": "5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/segunolalive/helloBooks.git"
  },
  "keywords": [
    "react",
    "library"
  ],
  "author": "segun ola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/segunolalive/helloBooks/issues"
  },
  "homepage": " https://segunolalive.github.io/helloBooks/templates/",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "1.0.2",
    "body-parser": "^1.17.2",
    "css-loader": "^0.28.5",
    "deepclonejs": "^1.0.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "faker": "^4.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jsonwebtoken": "^7.4.2",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "pg": "^7.0.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-materialize": "^1.0.10",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.4.2",
    "sequelize-cli": "^2.8.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "supertest": "^3.0.0",
    "webpack-dev-server": "^2.6.1"
  }
}
